{
    "name": "Auto Utilities Vein Mining",
    "author": "TheWii",
    "description": "Project description",
    "output": "./out",
    "data_pack": {
        "load": ["src"]
    },
    "require": [
        "mecha.contrib.bolt",
        "plugins.load_config"
    ],
    "pipeline": [
        "mecha",
        "plugins.remove_empty_functions",
        "beet.contrib.yellow_shulker_box"
    ],
    "meta": {
        "version": [ 4, 1, 0 ],
        "generate_namespace": "thewii",
        "generate_prefix": "autominer",
        "prefix": "twvm",
        "config": {
            "mining_levels": [ 0, 1, 2, 3, 4, 5],
            "tags": [
                "coal_ores",
                "iron_ores",
                "copper_ores",
                "gold_ores",
                "lapis_ores",
                "redstone_ores",
                "diamond_ores",
                "emerald_ores"
            ]    
        }
    }
}